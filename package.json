{
  "name": "vscode-openai-assistant",
  "displayName": "OpenAI Assistant",
  "description": "An extension for interacting with OpenAI through various commands.",
  "version": "0.0.4",
  "publisher": "Maccu",
  "repository": {
    "type": "git",
    "url": "https://github.com/mjmac87/vscode-openai-assistant.git"
  },
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.optimizeFileWithOpenAI",
        "title": "OpenAI Assistant: Optimize"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "OpenAI Assistant",
      "properties": {
        "openaiAPI.apiKey": {
          "type": "string",
          "default": "",
          "description": "Your OpenAI API Key"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.77.0",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "axios": "^1.3.5"
  }
}
